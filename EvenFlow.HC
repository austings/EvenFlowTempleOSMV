U0 RenderImage(CTask *,CDC *dc)
{
  dc->flags|=DCF_TRANSFORMATION;
  Mat4x4Scale(dc->r,glbl_scale);
  GrBlot3(dc,90,4+yOffset,0,cafe_dc[winmgr.updates%DC_NUM]);
}

U0 InitImage(U8 *filename,F64 scale)
{
  I64 i;
  glbl_scale=scale;
  for (i=0;i<DC_NUM;i++)
  {
    if(folder==3||folder==4)
    {
     cafe_dc[i]=BMPRead(filename,FALSE);
    }
    else
    {
     cafe_dc[i]=BMPRead(filename,TRUE);
    }
  }
}

U0 CleanUpImage()
{
  I64 i;
  //SettingsPop;
  
  for (i=0;i<DC_NUM;i++)
    DCDel(cafe_dc[i]);
}

U0 BMP24View(CDC* dc,CTask *task)
{
  RenderImage(task,dc);
  
}

U0 DrawIt(CTask *task, CDC *dc)
{
 I64 i;
 F64 scale=1;
 //F64 dt=framerule-190;//Beat-100;//Beat-t0;
 switch(colorRotation)
 {
  case 0:
   dc->color = BLUE;
   break;
  case 1:
   dc->color = LTBLUE;
   break;
  case 2:
   dc->color = CYAN;
   break;
  case 3:
   dc->color = LTCYAN;
   break;
  case 4:
   dc->color = LTGREEN;
   break;
  case 5:
   dc->color = GREEN;
   break;
 }
 if(colorFrame==10)
 {
  colorRotation++;
  colorFrame=0;
 }
 colorFrame++;
 if(colorRotation==6)
  colorRotation=0;
 if(colorChange==FALSE)
  dc->color = BLUE;

 GrRect(dc,0,0+yOffset,640,270);
 if(yOffset>0)
 {
  for(i=-20;i<680;i=i+25)
  {
   Sprite3B(dc,i+xOffset,yOffset,0,$IB,"<1>",BI=1$);
  }
  xOffset=xOffset+1;
  if(xOffset==25)
   xOffset=0;
 }

 if(butterflyOn)
 {
  if(butterflyState)
  {
   Sprite3B(dc,butterflyX,butterflyY,-10,$ER$$IB,"<2>",BI=2$);
   Sprite3B(dc,butterfly2X-2,butterfly2Y,-10,$IB,"<3>",BI=3$);

  }
  else
  {
   Sprite3B(dc,butterflyX-2,butterflyY,-10,$ER$$IB,"<3>",BI=3$);
   Sprite3B(dc,butterfly2X,butterfly2Y,-10,$ER$$ER$$IB,"<2>",BI=2$);

  }

  butterflyFrame++;
  if(butterflyFrame==20)
  {
   butterflyState=!butterflyState;
   butterflyFrame=0;
  }
 }

  if(butterflyOn){
    butterflyY--;
    butterfly2Y--;
  }
 
 if(framecounter==0)
 {
  if((imIndex>260&&folder==1) ||(imIndex>388&&folder==2) ||
     (imIndex>658&&folder==3) ||(imIndex>908&&folder==4) ||
     (imIndex>1142&&folder==5)||(imIndex>1410&&folder==6)||
     (imIndex>1902&&folder==7)||(imIndex>2084&&folder==8)||
     (imIndex>2196&&folder==9)||(imIndex>2498&&folder==10)||
     (imIndex>2602&&folder==11)||(imIndex>2866&&folder==12)||
     (imIndex>3054&&folder==13)||(imIndex>3458&&folder==14)||
     (imIndex>3710&&folder==15)||(imIndex>4032&&folder==16)||
     (imIndex>4156&&folder==17))
  {
    if(folder==17)
    {
     folder=1;
     imIndex=0;
    }
    else
    {
     if(folder==5)//accidently missing two images
     {
      imIndex=imIndex+4;
     }
     folder++;
    }
  }
  else
  {
   if(!first)
   {

    CleanUpImage();    
   }
   else
   {
     first=FALSE;
   }  
   U8* edit;
   edit =MStrPrint("%s%s%d%s%d%s",dir,"/Video/",folder,"/jesus-",imIndex,".bmp");
   imIndex=imIndex+2;
   InitImage(edit,scale);
  }
  if(folder==2||folder==5||folder==7||folder==9||folder==11||folder==16)
  {
  framecounter=1;
  }
  else{
  framecounter=2;
  }
 }
 else
 {
   if(butterflyY<-70)
   {
     butterflyY= 220;
   }
   if(butterfly2Y<-70)
   {
     butterfly2Y= 560;
   }
   framecounter--;
 }
 BMP24View(dc,task);
}

